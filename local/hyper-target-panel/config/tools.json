[
  {
    "id": "nmap",
    "name": "Nmap Basic",
    "command": "nmap -sV -sC -T4 {target} -oN {log_file}",
    "category": "Recon",
    "parser": "nmap",
    "presets": [
      { "name": "Quick", "command": "nmap -T4 -F {target} -oN {log_file}", "description": "Top 100 ports" },
      { "name": "Full", "command": "nmap -p- -T4 {target} -oN {log_file}", "description": "All 65535 ports" },
      { "name": "Vuln", "command": "nmap -sV --script vuln {target} -oN {log_file}", "description": "Check for vulnerabilities" },
      { "name": "UDP", "command": "sudo nmap -sU --top-ports 100 {target} -oN {log_file}", "description": "Top 100 UDP ports" }
    ]
  },
